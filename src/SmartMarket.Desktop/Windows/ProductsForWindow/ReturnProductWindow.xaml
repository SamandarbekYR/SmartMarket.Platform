<Window x:Class="SmartMarket.Desktop.Windows.ProductsForWindow.ReturnProductWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SmartMarket.Desktop.Windows.ProductsForWindow"
        xmlns:md="http://materialdesigninxaml.net/winfx/xaml/themes"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        WindowStyle="None"
        ResizeMode="NoResize"
        AllowsTransparency="True"
        Background="Transparent"
        Title="ReturnProductWindow"
        WindowState="Maximized"
        Loaded="Window_Loaded">
    
    <Border
        Height="640" 
        Width="1200"
        CornerRadius="8" BorderThickness="1" BorderBrush="Black" Background="White"> 
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="45"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>

            <Border
                   Grid.Row="0"
                   Margin="3 3 3 0"
                   BorderBrush="Black"
                   CornerRadius="5 5 0 0"
                   BorderThickness="1"
                   Background="#F61A0B"
                   >
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.8*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="35"/>
                    </Grid.ColumnDefinitions>

                    <TextBlock Grid.Column="0"
                              Text="Qaytarish"
                              VerticalAlignment="Center"
                              Margin="15 0 0 0"
                              FontSize="20"
                              FontWeight="DemiBold"
                               Foreground="White"
                              ></TextBlock>

                    <DockPanel Grid.Column="1">
                        <TextBlock Text="Dan:"
                               VerticalAlignment="Center"
                               FontSize="18"
                               Foreground="White"
                               ></TextBlock>
                        <Border Margin="5 5 15 5"
                                Background="White"
                                CornerRadius="5"
                                >
                            <DatePicker
             
                                  VerticalAlignment="Center"
                                  Margin="10 0 0 0"
                                  FontSize="18"
                                  BorderThickness="0"
                                  ></DatePicker>
                        </Border>
                    </DockPanel>


                    <DockPanel Grid.Column="2">
                        <TextBlock Text="gacha:"
                                  VerticalAlignment="Center"
                                  FontSize="18"
                                  Foreground="White"
                                  ></TextBlock>
                        <Border Margin="5 5 15 5"
                               Background="White"
                               CornerRadius="5"
                               >
                            <DatePicker
                                  Text="20/08/2024"
                                  VerticalAlignment="Center"
                                  Margin="10 0 0 0"
                                  FontSize="18"
                                  BorderThickness="0"
                                  ></DatePicker>
                        </Border>
                    </DockPanel>

                    <DockPanel Grid.Column="3">
                        <TextBlock Text="Sotuvchi:"
                                 VerticalAlignment="Center"
                                 FontSize="18"
                                 Foreground="White"
                                 ></TextBlock>
                        <Border Margin="5 5 15 5"
                               Background="White"
                               CornerRadius="5"
                               >
                            <TextBox
                                    Text="Azizjon"
                                    VerticalAlignment="Center"
                                    Margin="10 0 0 0"
                                    FontSize="18"
                                    BorderThickness="0"
                                    ></TextBox>
                        </Border>
                    </DockPanel>

                    <Border Grid.Column="4"
                            Margin="10 5"
                            CornerRadius="5"
                            Background="White"
                            >
                        <DockPanel>
                            <md:PackIcon Kind="Search"
                                    VerticalAlignment="Center"
                                    Margin="5 0 0 0"
                                    ></md:PackIcon>
                            <TextBox
                                    VerticalAlignment="Center"
                                    Margin="5 0 0 5"
                                    BorderThickness="0"
                                    FontWeight="SemiBold"
                                    Padding="0 2 0 0"
                                    ></TextBox>
                        </DockPanel>
                    </Border>
                    <Button Grid.Column="5"
                            x:Name="btnClose"
                            Click="btnClose_Click"
                            Style="{DynamicResource btnClose}">
                        
                    </Button>
                </Grid>
            </Border>

            <Grid Grid.Row="1">
                <Grid.RowDefinitions>
                    <RowDefinition Height="35"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid Grid.Row="0">

                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.5*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="1.2*"/>
                        <ColumnDefinition Width="1.2*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="0.5*"/>
                        <ColumnDefinition Width="1.5*"/>
                        <ColumnDefinition Width="1.5*"/>
                        <ColumnDefinition Width="0.5*"/>
                        <ColumnDefinition Width="1.5*"/>
                        <ColumnDefinition Width="1.5*"/>
                    </Grid.ColumnDefinitions>

                    <TextBlock
                                     Grid.Column="0"
                                     Text="N%"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Center"
                                     FontSize="15"
                                     FontWeight="SemiBold"
                                     ></TextBlock>

                    <TextBlock
                                     Grid.Column="1"
                                     Text="Transaction No"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Center"
                                     FontSize="15"
                                     FontWeight="SemiBold"
                                     ></TextBlock>

                    <TextBlock
                                     Grid.Column="2"
                                     Text="Mahsulot nomi"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Center"
                                     FontSize="15"
                                     FontWeight="SemiBold"
                                     ></TextBlock>

                    <TextBlock
                                     Grid.Column="3"
                                     Text="Barcode"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Center"
                                     FontSize="15"
                                     FontWeight="SemiBold"
                                     ></TextBlock>

                    <TextBlock
                                     Grid.Column="4"
                                     Text="Kateogoriya"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Center"
                                     FontSize="15"
                                     FontWeight="SemiBold"
                                     ></TextBlock>

                    <TextBlock
                                     Grid.Column="5"
                                     Text="Narxi"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Center"
                                     FontSize="15"
                                     FontWeight="SemiBold"
                                     Margin="0 0 10 0"
                                     ></TextBlock>

                    <TextBlock
                                     Grid.Column="6"
                                     Text="Chegirma"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Center"
                                     FontSize="15"
                                     FontWeight="SemiBold"
                                     Margin="0 0 5 0"
                                     ></TextBlock>
                    <TextBlock
                                     Grid.Column="7"
                                     Text="Soni"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Center"
                                     FontSize="15"
                                     FontWeight="SemiBold"
                                     Margin="0 0 5 0"
                                     ></TextBlock>

                    <TextBlock
                                     Grid.Column="8"
                                     Text="Jami"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Center"
                                     FontSize="15"
                                     FontWeight="SemiBold"
                                     Margin="0 0 15 0"
                                     ></TextBlock>

                    <TextBlock
                                     Grid.Column="9"
                                     Text="Sotuvchi"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Center"
                                     FontSize="15"
                                     FontWeight="SemiBold"
                                     Margin="0 0 15 0"
                                     ></TextBlock>

                    <TextBlock
                                     Grid.Column="10"
                                     Text="Kassa"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Center"
                                     FontSize="15"
                                     FontWeight="SemiBold"
                                     Margin="0 0 0 0"
                                     ></TextBlock>

                    <TextBlock
                                     Grid.Column="11"
                                     Text="Sana"
                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Center"
                                     FontSize="15"
                                     FontWeight="SemiBold"
                                     Margin="0 0 15 0"
                                     ></TextBlock>

                </Grid>
                <Border Grid.Row="1">
                    <ScrollViewer>
                        <StackPanel x:Name="St_ReturnProduct">
                            
                        </StackPanel>
                    </ScrollViewer>
                    
                </Border>
            </Grid>
        </Grid>
    </Border>
    
</Window>
